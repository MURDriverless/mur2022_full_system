<launch>
    
    <!--- Sim Time -->
    <param name="/use_sim_time" value="true" />

    <!--- Run Rviz-->
    <node pkg="rviz" type="rviz" name="rviz" args="-d $(find lego_loam_bor)/launch/test.rviz" />

    <!--- TF -->
    <node pkg="tf" type="static_transform_publisher" name="husky_to_camera"  args="0 0 0 1.570795   0        1.570795 /husky  /camera 10" />
    <node pkg="tf" type="static_transform_publisher" name="basler_to_camera" args="0 0 0 -1.570795 -1.570795 0        /basler /camera   10" />

<launch/>